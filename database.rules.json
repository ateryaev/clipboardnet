{
  "rules": {
    ".read": "auth != null",
    ".write": "auth != null",
    "clips": {
      ".indexOn": "createdBy",
      "$clipid": {
        ".write": "(!data.exists() && newData.child('createdBy').val() === auth.uid ) || (data.child('createdBy').val() === auth.uid && newData.child('createdBy').val() === auth.uid)"
      }
    },
    "users": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    }
  }
}